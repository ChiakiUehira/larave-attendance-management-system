{
  "name": "adonis-app",
  "version": "3.2.1",
  "main": "server.js",
  "engines": {
    "node": "8.4.0"
  },
  "scripts": {
    "start": "echo \"\\033[31mWARN: 'start' will be depreciated soon. Instead use 'npm run serve'\" && npm run serve",
    "dev": "echo \"\\033[31mWARN: 'dev' will be depreciated soon. Instead use 'npm run serve:dev'\" && npm run serve:dev",
    "serve:dev": "nodemon --watch app --watch resources --watch bootstrap --watch config --watch .env -x node server.js",
    "serve": "NODE_ENV=production node server.js",
    "build": "node ./ace nuxtbuild",
    "nyan": "ava --tap | tap-nyan",
    "test": "ava --verbose",
    "lint": "eslint --ext .js,.vue .",
    "fixer": "eslint --fix --ext .js,.vue .",
    "swagger": "json-refs resolve ./document/app.yml > ./document/app.json"
  },
  "author": "",
  "license": "MIT",
  "description": "",
  "private": true,
  "dependencies": {
    "adonis-ace": "^3.0.8",
    "adonis-auth": "^1.0.7",
    "adonis-commands": "^2.1.5",
    "adonis-fold": "^3.0.3",
    "adonis-framework": "^3.0.14",
    "adonis-lucid": "^3.0.16",
    "adonis-mail-provider": "^2.0.2",
    "adonis-middleware": "^1.0.14",
    "adonis-validation-provider": "^3.0.5",
    "adonis-websocket": "^1.0.4",
    "adonis-websocket-client": "^1.0.2",
    "axios": "^0.16.2",
    "chart.js": "^2.7.1",
    "cookie": "^0.3.1",
    "csv": "^1.2.1",
    "dotenv": "^4.0.0",
    "element-ui": "^2.0.3",
    "es6-promise": "^4.1.1",
    "github-markdown-css": "^2.9.0",
    "jimp": "^0.2.28",
    "js-cookie": "^2.2.0",
    "json-refs": "^3.0.2",
    "marked": "^0.3.6",
    "moment": "^2.19.1",
    "nuxt": "^1.0.0-rc9",
    "path": "^0.12.7",
    "pg": "^7.4.0",
    "rand-token": "^0.3.0",
    "vue": "^2.5.3",
    "vue-awesome": "^2.3.4",
    "vue-chartjs": "^3.0.2",
    "vue-server-renderer": "^2.5.3",
    "vue-template-compiler": "^2.5.3",
    "webpack-node-externals": "^1.6.0",
    "youch": "^2.0.5"
  },
  "ava": {
    "files": [
      "tests/api/**/*.js",
      "tests/web/*.js"
    ]
  },
  "autoload": {
    "App": "./app"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^6.0.0",
    "babel-preset-env": "^1.5.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^4.10.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "beta",
    "file-loader": "^0.9.0",
    "nodemon": "^1.11.0",
    "standard": "^10.0.1",
    "tap-nyan": "^1.1.0",
    "webpack": "^2.6.1",
