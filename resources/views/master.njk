<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

  <title>AdonisJs - Node.Js MVC Framework</title>

  <link href='http://fonts.googleapis.com/css?family=Source+Sans+Pro:400,200,300,600,700,900' rel='stylesheet' type='text/css'>
  <link rel="icon" href="/assets/favicon.png" type="image/x-icon">
</head>
<body>

  <section>
    <div class="__wrapper">
      <div id="app">
      {% block content %}{% endblock %}
      </div>
      {% block footer %}
        <div class="__footer">
          <ul>
            <li>
              <a href="https://twitter.com/intent/tweet?url=http://adonisjs.com&text=I am using AdonisJs, a practical MVC framework for nodejs&hashtags=nodejs,adonisframework" target="_blank">
                <img src="/assets/twitter.svg">
              </a>
            </li>
            <li>
              <a href="https://github.com/adonisjs/adonis-framework" target="_blank"><img src="/assets/github.svg"></a>
            </li>
          </ul>
        </div>
        {% endblock %}
        <script type=text/javascript src=/build.js></script>
    </div>
  </section>
</body>
</html>
